{
  "expo": {
    "name": "mobileapp",
    "slug": "mobileapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Plant.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Plant.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fakhri.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Plant.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fakhri.app"
    },
    "web": {
      "favicon": "./assets/Plant.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "a2419f1a-7442-40a2-98f4-ec58e71b4c68"
      }
    }
  }
}
